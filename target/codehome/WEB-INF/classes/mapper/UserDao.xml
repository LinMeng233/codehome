<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.huanyupiaoling.codehome.dao.UserDao">

    <select id="selectUser" resultType="User" parameterType="long">
       SELECT * FROM user WHERE id = #{id}
   </select>
    <insert id="insertUser"  parameterType="User">
<!--        <selectKey resultType="long" keyProperty="id" order="BEFORE">-->
<!--            SELECT ACCOUNT_SEQ.NEXTVAL FROM dual-->
<!--        </selectKey>-->
        INSERT INTO user(id,email,password,username,role,status,regTime,regIp,data)
        VALUES (#{id},#{email},#{password},#{username},#{role},#{status},#{regTime},#{regIp},#{data})
    </insert>



</mapper>